{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        ".output/**",
        ".histoire/**"
      ]
    },
    "generate" : {
      "dependsOn": ["^generate"],
      "outputs": [
        "dist/**",
        ".output/**"
      ]
    },  
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "codegen": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },   
    "test": {
      "outputs": []
    },
    "test:watch": {
      "outputs": []
    },    
    "test:turbo": {
      "outputs": []
    },
    "type-check": {}
  }
}
